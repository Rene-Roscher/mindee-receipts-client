{
    "name": "rene-roscher/mindee-receipts-client",
    "description": "mindee-receipts-client",
    "keywords": [
        "rene-roscher",
        "mindee-receipts-client"
    ],
    "homepage": "https://github.com/rene-roscher/mindee-receipts-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Rene Roscher",
            "email": "r.roscher@r-services.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "RServices\\MindeeReceiptsClient\\": "src"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RServices\\MindeeReceiptsClient\\MindeeReceiptsClientServiceProvider"
            ],
            "aliases": {
                "MindeeReceiptsClient": "RServices\\MindeeReceiptsClient\\MindeeReceiptsClientFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
